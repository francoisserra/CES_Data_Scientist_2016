Pour author, les noms et prénoms sont séparés par une vigule, et les différents auteurs par AND

@Article{hog,
	author={Dalal, Navneet AND Triggs, Bill},
	journal={cvpr},
	title={Histograms of Oriented Gradients for Human Detection},
	year={2005},
	month={jun},
	notes={sympathique}
}

@misc{hog2,
	author={Levi, Gil},
	title={A short introduction to descriptors},
	month={aug},
	year={2013},
	note={https://gilscvblog.com}
}

@misc{ref,
	author={Essid, Slim},
	title={Resources},
	year={2016},
	note={www.perso.telecom-paristech.fr/~essid/}
}

@article{DBLP:journals/corr/YosinskiCBL14,
	  author    = {Jason Yosinski and Jeff Clune and Yoshua Bengio and Hod Lipson},
	  title     = {How transferable are features in deep neural networks?},
	  journal   = {CoRR},
	  volume    = {abs/1411.1792},
          year      = {2014},
          url       = {http://arxiv.org/abs/1411.1792},
          timestamp = {Mon, 01 Dec 2014 14:32:13 +0100},
	  biburl    = {http://dblp.uni-trier.de/rec/bib/journals/corr/YosinskiCBL14},
	  bibsource = {dblp computer science bibliography, http://dblp.org}
}

@inproceedings{imagenet_cvpr09,
	AUTHOR = {Deng, J. and Dong, W. and Socher, R. and Li, L.-J. and Li, K. and Fei-Fei, L.},
	TITLE = {{ImageNet: A Large-Scale Hierarchical Image Database}},
	BOOKTITLE = {CVPR09},
	YEAR = {2009},
	BIBSOURCE = "http://www.image-net.org/papers/imagenet_cvpr09.bib"}

